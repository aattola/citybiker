    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.2.0",
    "config": "*",
    "eslint": "^8.12.0",
    "prisma": "^4.8.0",
    "rimraf": "^3.0.2",
    "tsconfig": "*",
    "tsup": "^5.11.13",
    "tsx": "^3.7.1",
